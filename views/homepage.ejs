<div ng-controller="PostController">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>User</th>
                <th>Content</th>
                <th>Votes</th>
                <th>Tags</th>
            </tr>
        </thead>
        <tbody>
            <tr class="" ng-repeat="post in posts.posts">
                <td>{{post.user}}</td>
                <td>{{post.content}}</td>
                <td>{{post.votes}}</td>
                <td>{{post.tags}}</td>
            </tr>
        </tbody>
    </table>
</div>
